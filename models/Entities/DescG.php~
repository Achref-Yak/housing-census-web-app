<?php

namespace Entities;

use Doctrine\ORM\Mapping as ORM;

/**
 * Entities\DescG
 */
class DescG
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var string $Rlegere
     */
    private $Rlegere;

    /**
     * @var string $RemiseEtat
     */
    private $RemiseEtat;

    /**
     * @var string $Bcontrole
     */
    private $Bcontrole;

    /**
     * @var Entities\Immeuble
     */
    private $Immeuble;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set Rlegere
     *
     * @param string $rlegere
     * @return DescG
     */
    public function setRlegere($rlegere)
    {
        $this->Rlegere = $rlegere;
        return $this;
    }

    /**
     * Get Rlegere
     *
     * @return string 
     */
    public function getRlegere()
    {
        return $this->Rlegere;
    }

    /**
     * Set RemiseEtat
     *
     * @param string $remiseEtat
     * @return DescG
     */
    public function setRemiseEtat($remiseEtat)
    {
        $this->RemiseEtat = $remiseEtat;
        return $this;
    }

    /**
     * Get RemiseEtat
     *
     * @return string 
     */
    public function getRemiseEtat()
    {
        return $this->RemiseEtat;
    }

    /**
     * Set Bcontrole
     *
     * @param string $bcontrole
     * @return DescG
     */
    public function setBcontrole($bcontrole)
    {
        $this->Bcontrole = $bcontrole;
        return $this;
    }

    /**
     * Get Bcontrole
     *
     * @return string 
     */
    public function getBcontrole()
    {
        return $this->Bcontrole;
    }

    /**
     * Set Immeuble
     *
     * @param Entities\Immeuble $immeuble
     * @return DescG
     */
    public function setImmeuble(\Entities\Immeuble $immeuble = null)
    {
        $this->Immeuble = $immeuble;
        return $this;
    }

    /**
     * Get Immeuble
     *
     * @return Entities\Immeuble 
     */
    public function getImmeuble()
    {
        return $this->Immeuble;
    }
}